<h1>Listing admin_groups</h1>
<div id="table_container">
  <table class="data_table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Period</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
    <% @admin_groups.each do |admin_group| %>
      <tr>
        <td><%= admin_group.name %></td>
        <td><%= admin_group.period %></td>
        <td><%= link_to 'Show', [:admin, admin_group] %></td>
        <td><%= link_to 'Edit', edit_admin_group_path(admin_group) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_group], confirm: 'Are you sure?', method: :delete %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
<br />

<%= link_to 'New Group', new_admin_group_path, class:"button success"   %>
